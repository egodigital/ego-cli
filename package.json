{
  "name": "ego-cli",
  "displayName": "e.GO CLI",
  "version": "0.88.0",
  "description": "Command Line Interface, which is designed to handle things, like Dev(Ops) tasks, much faster.",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "build": "del ./dist && tsc && (mkdir \"./dist/res\") && (cp -r ./src/res/* ./dist/res)",
    "doc": "del ./doc && (typedoc --out ./doc ./src)",
    "deploy": "npm install && npm audit fix && npm run build && npm publish"
  },
  "bin": {
    "ego": "./bin/ego"
  },
  "files": [
    "bin",
    "dist"
  ],
  "keywords": [
    "cli",
    "command",
    "file",
    "docker",
    "generator",
    "git",
    "http",
    "server",
    "share",
    "line",
    "interface",
    "task",
    "yeoman",
    "yo"
  ],
